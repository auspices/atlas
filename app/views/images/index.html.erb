<nav class='breadcrumb'>
  <%= link_to @user, user_collections_path(@user) %>
  <span class='breadcrumb-separator'></span>
  <span class='breadcrumb-current'><%= @collection || 'All images' %></span>
</nav>

<% if authorable? %>
  <%= render 'images/form' %>
<% end %>

<%= render 'images/images', images: @images %>

<div class='l-pagination'>
  <%= paginate @images %>
</div>
